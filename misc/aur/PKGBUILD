pkgname="gutdeb"
pkgver=0.0.1
pkgrel=1
url="https://github.com/gutenye/gutdeb"
pkgdesc="FIXME"
arch=("any")
license=("MIT")
source=(
  "$pkgname-$pkgver.tar.gz::https://github.com/gutenye/gutdeb/archive/v$pkgver.tar.gz"
)
md5sums=()

package() {
  cd "$srcdir/$pkgname-$pkgver"
  ./ake package "$pkgdir"
}

